package boltwise;

import java.util.Map;
import java.util.HashMap;

/**
 * @author Darrin Donahue
 * Class to process the FastenerData
 */
public class FastenerDataProcessor {
	// Map of our data with SKU as our key
	private Map<String, FastenerData> map = new HashMap<>();
	
	/**
	 * Insert an item.  Check the SKU to see if it already exists
	 */
	public boolean insertData(String SKU, FastenerData fastenerData) {
		map.put(SKU, fastenerData);
		return true;
	}

	// Convert to a List to return??
	public Map<String, FastenerData> getAllData() {
		return map;
	}
}
